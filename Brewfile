# Install dependencies via brew bundle
# https://github.com/Homebrew/homebrew-bundle

tap "heroku/brew"
brew "heroku"
brew "wget"
brew "caddy"
brew "nss" # use when running "caddy trust"
brew "dnsmasq"
brew "postgresql@13" # provides pgdump, psql
brew "zsh"
brew "asdf"
brew "python@3.9" # asdf (Node 14.X needs python3.9)
brew "gpg" # asdf (yarn needs gpg installed)
